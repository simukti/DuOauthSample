{
    "name": "simukti/du-oauth",
    "description": "ZF2 module provide user authentication using combination of Twitter OAuth, database checking, and Two-Factor-Authentication (Duo Security).",
    "type": "library",
    "keywords": [
        "zf2", "tfa", "duo auth", "twitter oauth"
    ],
    "homepage": "https://github.com/simukti/DuOauth",
    "authors": [
        {
            "name": "Sarjono Mukti Aji",
            "email": "me@simukti.net",
            "homepage": "http://www.simukti.net/"
        }
    ],
    "require": {
        "php": ">=5.3.10",
        "zendframework/zendframework": "2.*",
        "zendframework/zendservice-twitter": "2.*"
    },
    "autoload": {
        "psr-0": {
            "DuOauth": "src/"
        },
        "classmap": [
            "./Module.php"
        ]
    }
}
